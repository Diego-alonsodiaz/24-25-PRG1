import java.util.Scanner;

public class CalculadoraNotas {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Solicitar las notas al usuario
        System.out.println("Ingrese la nota del examen final (50%):");
        double examenFinal = scanner.nextDouble();

        System.out.println("Ingrese la nota del examen continuo (20%):");
        double examenContinuo = scanner.nextDouble();

        System.out.println("Ingrese la nota del examen parcial (25%):");
        double examenParcial = scanner.nextDouble();

        System.out.println("Ingrese la evaluaci√≥n del profesor (5%):");
        double evaluacionProfesor = scanner.nextDouble();

        // Calcular la nota final
        double notaFinal = (examenFinal * 0.50) + (examenContinuo * 0.20) + (examenParcial * 0.25) + (evaluacionProfesor * 0.05);

        // Mostrar el resultado
        System.out.printf("La nota final es: %.2f%n", notaFinal);

        scanner.close();
    }
}
